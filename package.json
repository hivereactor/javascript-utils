{
  "name": "@hivereactor/utils",
  "version": "2.1.2",
  "description": "Utility functions for your JavaScript project.",
  "main": "lib/cjs/index.js",
  "module": "lib/es/index.js",
  "types": "lib/types/index.d.ts",
  "directories": {
    "doc": "docs",
    "lib": "lib",
    "test": "test"
  },
  "files": [
    "lib/"
  ],
  "dependencies": {},
  "devDependencies": {
    "ava": "^0.20.0",
    "nyc": "^11.0.3",
    "typescript": "^2.4.1"
  },
  "scripts": {
    "build": "npm run build:cjs && npm run build:es",
    "build:cjs": "tsc -d --declarationDir lib/types/",
    "build:es": "tsc --module ES6 --outDir lib/es/",
    "prebuild": "rm -rf lib/",
    "prepublishOnly": "npm run build",
    "pretest": "npm run build:cjs -- --inlineSourceMap",
    "test": "nyc ava --no-color --verbose"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hivereactor/javascript-utils.git"
  },
  "author": "Hivereactor (https://hivereactor.com) <hello@hivereactor.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hivereactor/javascript-utils/issues"
  },
  "homepage": "https://github.com/hivereactor/javascript-utils#readme",
  "nyc": {
    "exclude": [
      "test/**/*"
    ],
    "reporter": "lcov"
  }
}
